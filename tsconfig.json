{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "node",
      "@wdio/globals/types",
      "expect-webdriverio",
      "@wdio/mocha-framework"
    ],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "noImplicitAny": true
  },
  "include": [
    "node_modules/expect-webdriverio",
    "node_modules/expect-webdriverio/jest"
  ]
}
